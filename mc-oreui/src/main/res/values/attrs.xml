<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextButton">
        <attr name="style">
            <enum name="common" value="0" />
            <enum name="gray" value="1" />
            <enum name="green" value="2" />
            <!--<enum name="pink" value="3" />-->
            <enum name="purple" value="4" />
            <enum name="red" value="5" />
        </attr>
        <attr name="bold" format="boolean" />
        <attr name="checked" format="boolean" />
        <attr name="disabled" format="boolean" />
    </declare-styleable>
    <declare-styleable name="TextButtonToggleGroup">
        <attr name="mini" format="boolean" />
        <attr name="defaultPosition" format="integer" />
    </declare-styleable>
</resources>